{
  "name": "autocut-export-worker",
  "version": "1.0.0",
  "description": "FFmpeg export worker for AutoCut Pro",
  "main": "export-processor.ts",
  "scripts": {
    "start": "tsx export-processor.ts",
    "dev": "tsx --watch export-processor.ts",
    "build": "tsc export-processor.ts --outDir dist",
    "build:start": "npm run build && node dist/export-processor.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "archiver": "^6.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "ffmpeg-static": "^5.2.0",
    "pg": "^8.11.3",
    "dotenv": "^16.6.1",
    "@supabase/supabase-js": "^2.45.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/archiver": "^6.0.2",
    "@types/node": "^20.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
}